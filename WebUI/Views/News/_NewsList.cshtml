@model IEnumerable<Domain.Entities.News>

<div class="row">
    <div class="col-md-8 col-md-offset-1">
        <ul class="media-list">
            @foreach (var news in Model)
            {
                @Html.Partial("~/Views/News/_NewsSummary.cshtml", news)
            }
        </ul>
    </div>
</div>
